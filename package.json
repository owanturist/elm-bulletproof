{
  "name": "elm-bulletproof",
  "version": "1.0.0",
  "description": "Make your components Bulletproof",
  "main": "index.js",
  "scripts": {
    "test": "elm-test --compiler ./node_modules/.bin/elm",
    "predemo-build": "rm -rf build",
    "demo-build": "elm make --output=./build/index.html tests/Demo.elm",
    "demo-serve": "http-server ./build --proxy http://localhost:8080?",
    "demo": "npm run demo-build && npm run demo-serve",
    "demo:debug": "npm run demo-build -- --debug && npm run demo-serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/owanturist/elm-bulletproof.git"
  },
  "keywords": [
    "elm",
    "storybook"
  ],
  "author": "Anton Ovechkin <owanturist@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/owanturist/elm-bulletproof/issues"
  },
  "homepage": "https://github.com/owanturist/elm-bulletproof#readme",
  "devDependencies": {
    "elm": "^0.19.1-3",
    "elm-analyse": "^0.16.5",
    "elm-format": "^0.8.2",
    "elm-test": "^0.19.1-revision2",
    "http-server": "^0.12.1"
  },
  "directories": {
    "test": "tests"
  },
  "dependencies": {}
}
