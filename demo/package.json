{
  "name": "elm-bulletproof-demo",
  "version": "1.0.0",
  "description": "Bulletproof of Bulletproof",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -rf ../build && mkdir ../build && cp ./index.html ../build/",
    "build": "elm make --output=../build/bundle.js ./src/Stories.elm",
    "serve": "http-server ../build --proxy http://localhost:8080?",
    "demo": "npm run build && npm run serve",
    "demo:optimize": "npm run build -- --optimize && npm run serve",
    "demo:debug": "npm run build -- --debug && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/owanturist/elm-bulletproof.git"
  },
  "keywords": [
    "elm",
    "storybook"
  ],
  "author": "Anton Ovechkin <owanturist@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/owanturist/elm-bulletproof/issues"
  },
  "homepage": "https://github.com/owanturist/elm-bulletproof#readme",
  "devDependencies": {
    "elm": "^0.19.1-3",
    "http-server": "^0.12.1"
  }
}
